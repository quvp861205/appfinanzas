{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "./dist/myapp/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "./index.html"
      }
    ]
  }
}
